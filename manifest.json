{
    "version": "1.2.3",
    "manifest_version": 2,
    "name": "__MSG_appName__",
    "description": "__MSG_appDesc__",
    "default_locale": "zh_CN",
    "author": "New Future",
    "homepage_url": "https://newfuture.github.io/YYF-Debugger/",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "main.js"
            ],
            "run_at": "document_start"
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "icons": {
        "16": "icon/logo/16.png",        
        "48": "icon/logo/48.png",        
        "128": "icon/logo/128.png"
    },
    "browser_action": {
        "default_title": "YYF Debugger"
    },
    "permissions": [
        "webRequest",
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}